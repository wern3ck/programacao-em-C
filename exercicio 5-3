#include <stdio.h>
#include <stdlib.h>

int main()
{
    float soma, diferenca, produto, quociente, resto;
    int a = 11, b = 3;

    soma = a + b;
    diferenca = a - b;
    produto = a * b;
    quociente = a / b;
    resto = a % b;

    printf("\nos resultado sao: %.f, %.f, %.f, %.f, %.f", soma, diferenca, produto, quociente, resto);


    return 0;
}
